<scroll-view scroll-y class="scrollPage">
    <!-- <image src='/image/cander/title.png' mode='widthFix' class='response'></image> -->
    <view class="cu-bar justify-center bg-white">
        <view class="action border-title">
            <text class="text-bold">温馨提示：东校区 西校区 北校区 为同一作息时间</text>
            <text class="bg-cyan" style="width:5rem"></text>
        </view>
    </view>
    <view class='nav-list'>
        <navigator hover-class='none' url="/pages/home/showCalendar/{{item.name}}/{{item.name}}" class="nav-li bg-{{item.color}}" wx:for="{{elements}}" wx:key="index">
            <view class="nav-title">{{item.title}}</view>
            <view class="nav-name">{{item.name}}</view>
            <text class='cuIcon-{{item.icon}}'></text>
        </navigator>
    </view>
    <view class='cu-tabbar-height'></view>
</scroll-view>


